{
  "name": "Brexit Blocker",
  "description": "Block mentions of brexit",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["jquery.js", "blocker.js"],
     "run_at": "document_start"
    }
  ],
  "manifest_version": 2
}
